{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "soccer-project": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/robots.txt"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/style-bs.css",
              "src/assets/css/bootstrap-checkbox.css",
              "src/assets/css/obaju-theme.css",
              "node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/ladda/dist/ladda-themeless.min.css"
            ],
            "scripts": [
              "node_modules/tinymce/tinymce.js",
              "node_modules/tinymce/themes/modern/theme.js",
              "node_modules/tinymce/plugins/link/plugin.js",
              "node_modules/tinymce/plugins/paste/plugin.js",
              "node_modules/tinymce/plugins/table/plugin.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/datatables.net/js/jquery.dataTables.js"
            ]
          },
          "configurations": {
            "p": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.p.ts"
                }
              ]
            },
            "q": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.q.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "soccer-project:build"
          },
          "configurations": {
            "p": {
              "browserTarget": "soccer-project:build:p"
            },
            "q": {
              "browserTarget": "soccer-project:build:q"
            },
            "production": {
              "browserTarget": "soccer-project:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "soccer-project:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "scripts": [
              "node_modules/tinymce/tinymce.js",
              "node_modules/tinymce/themes/modern/theme.js",
              "node_modules/tinymce/plugins/link/plugin.js",
              "node_modules/tinymce/plugins/paste/plugin.js",
              "node_modules/tinymce/plugins/table/plugin.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/datatables.net/js/jquery.dataTables.js"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/style-bs.css",
              "src/assets/css/bootstrap-checkbox.css",
              "src/assets/css/obaju-theme.css",
              "node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "node_modules/font-awesome/css/font-awesome.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/ladda/dist/ladda-themeless.min.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/robots.txt"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "soccer-project-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "soccer-project:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "soccer-project",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}